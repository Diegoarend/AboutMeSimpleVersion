{% extends "layout.njk" %}

{% block head %}

    <title>Diego Arend</title>

{% endblock %}
   

{% block content %}

<div id="wrapper">
    <img src= "{{about.avatar_url}}" alt="diego">
    <h1>{{about.name}}</h1>
    <h2>{{about.role}}</h2>
    <p>{{about.description}}</p>   
</div>

<div class="links" id="links-footer">

{% for link in about.links%}
    <a href="{{link.url}}" target="_blank"> {{link.name}}</a>
   
{%endfor%}
</div>




{% endblock %}

